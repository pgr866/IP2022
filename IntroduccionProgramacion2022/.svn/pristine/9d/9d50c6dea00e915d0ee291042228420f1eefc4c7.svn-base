package org.ip.tema03.ejerciciosresueltos;

public class Piano extends Instrumento {

	public Piano() {
		super("Piano", "Teclado y cuerdas");
	}

	@Override
	public void interpretar() {
		System.out.println(nombre + " - " + tipo);
		for (int i = 0; i < numeroNotas; i++) {
			switch (notas[i]) {
			case DO:
				System.out.print("Do ");
				break;
			case RE:
				System.out.print("Re ");
				break;
			case MI:
				System.out.print("Mi ");
				break;
			case FA:
				System.out.print("Fa ");
				break;
			case SOL:
				System.out.print("Sol ");
				break;
			case LA:
				System.out.print("La ");
				break;
			case SI:
				System.out.print("Si ");
				break;
			}
		}
		System.out.println("");
	}
}
